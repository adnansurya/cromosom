<script>
    let page = '<%= page %>';

    if(page == 'work'){
        $('#nav_work').addClass('active work');
    }else if(page == 'about'){
        $('#nav_about').addClass('active about');
    }else if(page == 'contact'){
        $('#nav_contact').addClass('active contact');
    }else if(page == 'funwork'){
        $('#nav_funwork').addClass('active funwork');
    }


</script>