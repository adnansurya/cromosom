<!doctype html>
<html lang="id">
    <!-- Head Section -->
  <head>
    <%- include('templates/head'); -%>

  </head>
<!-- Head Section End -->
  
<body>

<!-- Header Section -->
<%- include('templates/header'); -%>
<!-- Header Section End -->

         <!-- Content Section -->
         <div class="container-fluid bg-light pb-5 my-4">
                <div class="container">
                    <!-- Project One -->
                    <div class="row pt-5">
                        <div class="col-md-8">
                            <img width="100%" height="50%" class="img-fluid rounded mb-3 mb-md-0" src="https://firebasestorage.googleapis.com/v0/b/cromosom1.appspot.com/o/work%2F<%= key %>%2F<%= workdetail.foto %>?alt=media" alt="<%= workdetail.nama %>" >
                        </div>
                        <div class="col-md-4">
                            <h3 class="reds" style="color:black"><%= workdetail.nama %></h3>
                            <p><%= workdetail.keterangan %></p>
                        </div>
                    </div>
                    <% var extra = workdetail.tambahan; console.log(extra) %>
                    <% Object.keys(extra).forEach(function(key_) { var val = extra[key_];%>
                        <div class="row pt-5">
                            <div class="col-md-8">
                                <% if (val.foto !== "") {%>
                                    <img  alt="<%= val.foto %>" width="100%" height="50%" class="img-fluid rounded mb-3 mb-md-0" s src="https://firebasestorage.googleapis.com/v0/b/cromosom1.appspot.com/o/work%2F<%= key %>%2F<%= val.foto %>?alt=media">
                                    <br>
                                <% } else {%>
                                    <p>Tidak Ada Foto</p>
                                <% } %>
                            </div>
                            <div class="col-md-4">
                                
                                <p><%= val.deskripsi %></p>
                            </div>
                        </div>
                    <%});%>
    
                   
                </div>
        </div>
            <!-- Content End -->

        <!-- Footer Section -->
        <%- include('templates/footer'); -%>
        <!-- Footer End -->


    </body>
</html>
