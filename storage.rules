service firebase.storage {
  match /b/{bucket}/o {
     match /foto/{fileName} {
      allow write: if request.auth != null;
      allow read;
    }

    match /work/{fileName} {
      allow write: if request.auth != null;
      allow read;
    }

     match /funwork/{fileName} {
      match /{allPaths=**}{
        allow write: if request.auth != null;
        allow read;
     }
    }

    match /{allPaths=**} {
     allow read, write: if request.auth!=null;
    }
  }
}
